<%= form_with(model: [@restaurant, @special_menu, @image], url: restaurant_special_menu_images_path(@restaurant, @special_menu), local: true) do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>
  
    <%= form.label :image %>
    <%= form.file_field :image %>
  
    <%= form.submit 'Upload Image' %>
<% end %>
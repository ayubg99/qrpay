<p id="notice"><%= notice %></p>

<h1>Special Menus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Restaurant</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @special_menus.each do |special_menu| %>
      <tr>
        <td><%= special_menu.name %></td>
        <td><%= special_menu.restaurant %></td>
        <td><%= link_to 'Edit', edit_restaurant_special_menu_path(special_menu, @restaurant) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

